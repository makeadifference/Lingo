MODEL:
SETS:
   NUTS / PEANUTS, CASHEWS/: SUPPLY;
   BRANDS / PAWN, KNIGHT, BISHOP, KING/:
    PRICE, PRODUCE;
   NCROSSB( NUTS, BRANDS): FORMULA;
ENDSETS

DATA:
   SUPPLY = 750 250;
   PRICE = 2 3 4 5;
   FORMULA = 15 10  6  2
              1  6 10 14;
ENDDATA

MAX = @SUM( BRANDS( I): 
 PRICE( I) * PRODUCE( I));

@FOR( NUTS( I): 
 @SUM( BRANDS( J): 
  FORMULA( I, J) * PRODUCE( J) / 16) <= 
   SUPPLY( I)
);
END
